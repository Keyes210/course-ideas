{{#partial "content"}}
    <h1>{{ idea.title }}</h1>
    <h2>{{ idea.voteCount }} Voters</h2>

    <ul>
        {{#each idea.voters}}
            <li>{{ this }}</li> <!--b/c voter is string not object you can use this to access current scope-->
        {{/each}}
    </ul>

    <form action="/ideas/{{ idea.slug }}/vote" method="post">
        <button>Vote</button>
    </form>
{{/partial}}

{{#partial "title"}}
    Course Idea - {{ idea.title }}
{{/partial}}
{{> base.hbs}}